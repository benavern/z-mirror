{
  "general": {
    "sound": true
  },
  "clock": {
    "showSeconds": true
  },
  "weather": {
    "api": {
      "key": "[[OPENWEATHERMAP_APP_ID]]",
      "baseUrl": "http://api.openweathermap.org/data/2.5/weather"
    },
    "location": "[[OPENWEATHERMAPCITYNAME]]",
    "defaultIcon": 950,
    "defaultIconPeriod": "day",
    "units": "metric",
    "updateInterval": 3600000
  },
  "forecast": {
    "api": {
      "key": "[[OPENWEATHERMAP_APP_ID]]",
      "baseUrl": "http://api.openweathermap.org/data/2.5/forecast/daily"
    },
    "location": "[[OPENWEATHERMAP_CITY_NAME]]",
    "defaultIcon": 950,
    "defaultIconPeriod": "day",
    "units": "metric",
    "limit": 5,
    "updateInterval": 3600000
  },
  "chuckNorris": {
    "api": {
      "baseUrl": "http://www.chucknorrisfacts.fr/api/get"
    },
    "orderby": "alea",
    "limit": 1,
    "updateInterval": 600000
  },
  "shopping": {
    "api": {
      "baseUrl": "[[Z_MIRROR_API]]/shopping"
    },
    "updateInterval": 900000
  },
  "postit": {
    "api": {
      "baseUrl": "[[Z_MIRROR_API]]/postit"
    },
    "updateInterval": 900000
  },
  "photos": {
    "api": {
      "baseUrl": "[[Z_MIRROR_API]]/photos"
    },
    "updateInterval": 900000,
    "carouselInterval": 30000
  }
}
